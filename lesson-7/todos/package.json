{
  "name": "idea-journal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-modal": "3.0.3",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "2.83.0",
    "shortid": "2.2.8"
  },
  "devDependencies": {
    "prettier": "1.7.4",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
